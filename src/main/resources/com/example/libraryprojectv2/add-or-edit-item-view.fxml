<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label layoutX="31.0" layoutY="85.0" text="Item Title" />
      <Label layoutX="31.0" layoutY="151.0" text="Item Author" />
      <Label fx:id="lblAddItemHeader" layoutX="31.0" layoutY="30.0" text="Add Item">
         <font>
            <Font size="27.0" />
         </font>
      </Label>
      <TextField fx:id="txtItemTitleInAddItem" layoutX="31.0" layoutY="108.0" prefHeight="39.0" prefWidth="275.0" promptText="Title">
         <font>
            <Font size="15.0" />
         </font></TextField>
      <TextField fx:id="txtItemAuthorInAddItem" layoutX="31.0" layoutY="175.0" prefHeight="39.0" prefWidth="275.0" promptText="Author">
         <font>
            <Font size="15.0" />
         </font></TextField>
      <Button fx:id="btnConfirmInAddItem" layoutX="31.0" layoutY="231.0" mnemonicParsing="false" onAction="#onClickConfirmInAddItemButton" prefHeight="32.0" prefWidth="196.0" text="CONFIRM" />
      <Button fx:id="btnCloseInAddItem" layoutX="31.0" layoutY="278.0" mnemonicParsing="false" onAction="#onClickCloseInAddItemButton" prefHeight="32.0" prefWidth="196.0" text="CLOSE" />
      <Label fx:id="lblInformationInAddItem" layoutX="31.0" layoutY="334.0" prefHeight="32.0" prefWidth="552.0" />
   </children>
</AnchorPane>
